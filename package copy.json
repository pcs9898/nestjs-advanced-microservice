{
  "name": "nestjs-advanced-cqrs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/nestjs-advanced-cqrs/main",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource ./src/database/data-source.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create ./src/database/migrations/Migration",
    "migration:generate": "yarn  typeorm migration:generate ./src/database/migrations/Migration",
    "migration:run": "yarn  typeorm  migration:run",
    "migration:revert": "yarn  typeorm migration:revert",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs seed:run",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config=jest.config.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/nestjs-advanced-cqrs/test/jest-e2e.json",
    "microserviceCopyPackageJsonDockerComposeNoCache": "cp ./package.json ./apps/api-gateway && cp ./package.json ./apps/user && cp ./package.json ./apps/video && cp ./package.json ./apps/analytics && cp ./package.json ./apps/health && cp ./package.json ./apps/mail && cp ./package.json ./apps/scheduled-batch &&  docker-compose -p microservice down && docker-compose -p microservice build --no-cache && docker-compose -p microservice up --remove-orphans",
    "microserviceCopyPackageJson": "cp ./package.json ./apps/api-gateway && cp ./package.json ./apps/user && cp ./package.json ./apps/video && cp ./package.json ./apps/analytics && cp ./package.json ./apps/health && cp ./package.json ./apps/mail && cp ./package.json ./apps/scheduled-batch &&  docker-compose -p microservice down && docker-compose -p microservice up --remove-orphans",
    "postgres": "docker-compose -f postgres.yml -p postgres down && docker-compose -f postgres.yml -p postgres up --remove-orphans",
    "kafka": "docker-compose -f kafka-2.8.1.yml -p kafka down && docker-compose -f kafka-2.8.1.yml -p kafka up --remove-orphans"
  },
  "dependencies": {
